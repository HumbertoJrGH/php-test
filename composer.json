{
    "name": "humbe/teste",
    "description": "teste php",
    "scripts": {
        "test": "php vendor/bin/phpunit --colors=always",
        "test:coverage": "php vendor/bin/phpunit --coverage-html coverage --colors=always",
        "test:watch": "php vendor/bin/phpunit --colors=always --watch"
    },
    "autoload": {
        "psr-4": {
            "Control\\": "Control/",
            "Model\\": "Model/",
            "View\\": "View/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "require": {
        "illuminate/database": "^12.16",
        "vlucas/phpdotenv": "^5.6"
    }
}